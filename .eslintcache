n              ))\r\n            }\r\n        </Row>\r\n      </Container>\r\n      {newPanel.render}\r\n      {resolveBusyConflictDialog.render}\r\n    </>\r\n  );\r\n};\r\n\r\nexport default TaskDashboard;\r\n","/app/src/components/navigation/components/app-bar/AppBar.jsx",[],"/app/src/components/navigation/components/drawer/Drawer.jsx",[],"/app/src/components/appraisal-list/panel-new/PanelNew.jsx",[],"/app/src/components/appraisal-list/panel-edit/PanelEdit.jsx",[],"/app/src/components/shared/page-header/index.js",[],"/app/src/routes/reporting-reports/index.js",[],"/app/src/components/shared/authorization-redirect-component/index.js",[],"/app/src/components/shared/table/index.js",[],"/app/src/components/shared/chip/index.js",[],"/app/src/routes/appraisal-details/index.js",[],"/app/src/routes/settings-general/index.js",[],"/app/src/routes/settings-users/index.js",[],"/app/src/routes/reporting-templates/index.js",[],"/app/src/routes/settings-role-permissions/index.js",[],"/app/src/routes/settings-app-items/index.js",[],"/app/src/routes/settings-app-periods/index.js",[],"/app/src/components/widgets/home-planned-items/index.js",[],"/app/src/styles/keytipStyles.js",[],"/app/src/components/shared/date-picker/DatePicker.jsx",[],"/app/src/components/navigation/components/user-info/UserInfo.jsx",[],"/app/src/components/ui-hooks/usePanel.jsx",[],"/app/src/services/validators/index.js",[],"/app/src/components/shared/page-header/PageHeader.jsx",[],"/app/src/components/shared/authorization-redirect-component/AuthorizationRedirectComponent.jsx",[],"/app/src/components/shared/table/Table.jsx",[],"/app/src/routes/reporting-reports/ReportsPage.jsx",[],"/app/src/routes/appraisal-details/AppraisalDetailsPage.jsx",[],"/app/src/components/shared/chip/Chip.jsx",[],"/app/src/routes/settings-role-permissions/SettingsRoles.jsx",[],"/app/src/routes/reporting-templates/ReportTemplatesPage.jsx",[],"/app/src/routes/settings-general/SettingsGeneral.jsx",[],"/app/src/routes/settings-users/Setti